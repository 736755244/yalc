{
  "extends": ["tslint-plugin-prettier", "tslint-config-prettier"],
  "rules": {
    "no-floating-promises": true,
    "no-unused-expression": true,
    "whitespace": [true, "check-brace"],
    "prettier": [
      true,
      {
        "semi": false,
        "singleQuote": true
      }
    ]
  }
}
